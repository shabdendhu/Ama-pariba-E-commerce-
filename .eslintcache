  .post(remove_product_from_basket, {\n    //     // product_id: id,\n    //     // product_qnt: parseInt(amount),\n    //     // user_id: user_info[0].id,\n    //     item_id: item_id,\n    //   })\n    //   .then((response) => {\n    //     if (response.data.status) {\n    //       setShowLoader(false);\n    //     }\n    //     // console.log(response.data.data);\n    //   });\n  };\n\n  const addToBasket = () => {\n    // console.log(isLoggedIn);\n    if (isLoggedIn) {\n      AddtoBasketApi();\n    } else {\n      window.location.href = \"/login\";\n    }\n    // console.log(basket);\n  };\n  const removeFromBasket = () => {\n    setItemAmt(itemAmt - 1);\n    dispacher({\n      type: \"REMOVE_FROM_BASKET\",\n      item_id: parseInt(item_id),\n    });\n    RemoveProductFromBasket();\n  };\n  useEffect(() => {\n    // if (stage === \"remove\") {\n    if (count) {\n      setItemAmt(count);\n    }\n\n    // console.log(name, count);\n    // }\n  }, [count]);\n  useEffect(() => {\n    setBasketItemId(item_id);\n  }, [item_id]);\n  return (\n    <>\n      {/* {itemAmt == 0 ? \"this\" : itemAmt == -1 ? \"\" : \"\"} */}\n      {itemAmt === 0 ? (\n        <button\n          onClick={stage === \"add\" ? addToBasket : removeFromBasket}\n          style={{\n            backgroundColor: \"#9e2121\",\n            marginBottom: \"5px\",\n            marginLeft: \"5px\",\n            height: \"35px\",\n            width: width,\n            color: \"white\",\n            border: \"none\",\n            outline: \"none\",\n            borderRadius: \"5px\",\n            fontSize: \"15px\",\n          }}\n        >\n          {stage === \"add\" ? \"ADD\" : \"REMOVE\"}\n        </button>\n      ) : (\n        <div\n          onClick={() => {\n            itemAmt === 1 && stage === \"remove\" && removeFromBasket();\n          }}\n          style={{\n            backgroundColor: \"#9e2121\",\n            marginBottom: \"5px\",\n            marginLeft: \"5px\",\n            height: \"35px\",\n            width: width,\n            color: \"white\",\n            border: \"none\",\n            outline: \"none\",\n            borderRadius: \"5px\",\n            fontSize: \"17px\",\n            display: \"flex\",\n            alignItems: \"center\",\n            justifyContent: \"space-around\",\n          }}\n        >\n          {itemAmt === 1 && stage === \"remove\" ? (\n            \"REMOVE\"\n          ) : (\n            <>\n              <IconButton\n                style={{ fontSize: \"24px\", fontWeight: 500, color: \"white\" }}\n                onClick={() => {\n                  addToBasket();\n                }}\n              >\n                <Add />\n              </IconButton>\n              <span style={{ fontSize: \"19px\", fontWeight: 500 }}>\n                {showLoader ? (\n                  <div\n                    style={{\n                      display: \"flex\",\n                      alignItems: \"center\",\n                      justifyContent: \"center\",\n                    }}\n                  >\n                    <CircularProgress\n                      style={{ width: \"25px\", height: \"25px\", color: \"white\" }}\n                    />\n                  </div>\n                ) : (\n                  <span>{itemAmt}</span>\n                )}\n              </span>\n              <IconButton\n                style={{ fontSize: \"24px\", fontWeight: 500, color: \"white\" }}\n                onClick={() => {\n                  removeFromBasket();\n                }}\n              >\n                <Remove />\n              </IconButton>\n            </>\n          )}\n        </div>\n      )}\n    </>\n  );\n};\n\nexport default CustomButton;\n","/home/kishan/amapariba/Ama-pariba-E-commerce-/src/Component/Product/Shopbyseasonsbest.js",["409","410","411","412"],"import { CircularProgress, Grid } from \"@material-ui/core\";\n// import axios from \"axios\";\nimport React from \"react\";\nimport { useState } fro